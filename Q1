A) select ename, sal from empl.empl where sal >= 2200;
B) select * from empl.empl where comm is null;
C) select ename, sal from empl.empl where sal not between 2500 and 4000;
D) select ename, job, sal from empl.empl where mgr is null;
E) select ename from empl.empl where ename like '__a%';
F) select ename from empl.empl where ename like '%t';
